{
    "name": "setup-typescript-and-node",
    "version": "1.0.0",
    "description": "This is a quick setup for projects with Node.js and TypeScript",
    "main": "src/server.ts",
    "scripts": {
        "start": "tsc && node dist/server.js",
        "start:dev": "tsx watch src/server.ts",
        "test": "vitest --passWithNoTests",
        "test:watch": "vitest dev --passWithNoTests",
        "test:coverage": "vitest --coverage",
        "husky:prepare": "husky install"
    },
    "keywords": [
        "typescript",
        "node.js",
        "setup"
    ],
    "author": "Julio Fiamoncini",
    "license": "MIT",
    "devDependencies": {
        "@types/cors": "^2",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.2",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "eslint": "^8.55.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.0.1",
        "git-commit-msg-linter": "^5.0.4",
        "husky": "^8.0.3",
        "lint-staged": "^15.1.0",
        "prettier": "^3.1.0",
        "supertest": "^6.3.4",
        "tsx": "^4.6.2",
        "typescript": "^5.3.2",
        "typescript-coverage-report": "^0.8.0",
        "vitest": "^1.2.1"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.4.0",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.35.2",
        "zod": "^3.22.4"
    }
}
